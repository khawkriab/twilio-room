{
  "name": "@pharmcare/twilio-room",
  "version": "0.0.8",
  "description": "Custom Twilio Video UI",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "npm pack",
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:tsc",
    "build:tsc": "tsc --module commonjs --outDir dist",
    "build:publish": "npm run build && npm publish",
    "prepack": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "twilio",
    "twilio-video",
    "twilio-room"
  ],
  "author": "Khawkriab Sittisak",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@react-spring/web": "^9.7.4",
    "@twilio/video-room-monitor": "^1.0.1",
    "clsx": "^2.0.0",
    "d3-timer": "^1.0.10",
    "is-plain-object": "^5.0.0",
    "lodash.throttle": "^4.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "twilio": "^5.3.1",
    "twilio-video": "^2.28.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/d3-timer": "^3.0.2",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "^18.19.50",
    "@types/react": "^18.3.8",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
