{
    "name": "twilio-room",
    "version": "1.0.0",
    "description": "custom twilio-video UI",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "scripts": {
        "clean": "rimraf ./dist",
        "start": "react-scripts start",
        "start:example": "npm pack && cd example && npm i --save ../twilio-room-wrapper-1.0.42.tgz && react-scripts start",
        "build": "npm run clean && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc --outDir dist/esm",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "build:publish": "npm run build && npm publish",
        "copyFiles:esm": "copyfiles -u 1 ./src/**/*.jpg ./dist/esm/",
        "copyFiles:cjs": "copyfiles -u 1 ./src/**/*.jpg ./dist/cjs/",
        "prepack": "npm run build"
    },
    "files": [
        "dist",
        "types"
    ],
    "exports": {
        ".": {
            "import": {
                "types": "./lib/esm/index.d.ts",
                "default": "./lib/index.mjs"
            },
            "require": {
                "types": "./lib/cjs/index.d.ts",
                "default": "./lib/cjs/index.js"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/khawkriab/twilio-room.git"
    },
    "keywords": [
        "twilio",
        "twilio-viode",
        "twilio-room"
    ],
    "author": "khawkriab",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/khawkriab/twilio-room/issues"
    },
    "dependencies": {
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.60",
        "@types/d3-timer": "^1.0.9",
        "@types/react": "^16.9.19",
        "d3-timer": "^1.0.10",
        "is-plain-object": "^5.0.0",
        "lodash": "^4.17.21",
        "react": "^16.12.0",
        "swiper": "^8.1.5",
        "twilio": "^3.63.1",
        "twilio-video": "^2.28.0",
        "typescript": "^4.6.2"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.199",
        "@types/node": "^20.8.6",
        "@types/react-dom": "^16.9.5",
        "copyfiles": "^2.4.1",
        "react-dom": "^16.12.0",
        "react-scripts": "^5.0.1",
        "rimraf": "^5.0.5"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
