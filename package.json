{
    "name": "twilio-room",
    "version": "1.0.10",
    "description": "custom twilio-video UI",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "scripts": {
        "clean": "rimraf ./dist",
        "dev": "npm pack && cd example && npm i --save ../%npm_package_name%-%npm_package_version%.tgz && cross-env FAST_REFRESH=false react-scripts start",
        "start": "cross-env FAST_REFRESH=false react-scripts start",
        "start:example": "cd example && cross-env FAST_REFRESH=false react-scripts start",
        "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run copyFiles:types",
        "build:esm": "tsc --outDir dist/esm",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "build:publish": "npm run build && npm publish",
        "copyFiles:esm": "copyfiles -u 1 ./src/**/*.jpg ./dist/esm/",
        "copyFiles:cjs": "copyfiles -u 1 ./src/**/*.jpg ./dist/cjs/",
        "copyFiles:types": "copyfiles -u 1 ./types/*.d.ts ./dist/types",
        "prepack": "npm run build"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.ts",
                "default": "./dist/esm/index.js"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/khawkriab/twilio-room.git"
    },
    "keywords": [
        "twilio",
        "twilio-viode",
        "twilio-room"
    ],
    "author": "khawkriab",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/khawkriab/twilio-room/issues"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.14",
        "@mui/material": "^5.14.14",
        "@mui/styles": "^5.14.14",
        "clsx": "^2.0.0",
        "d3-timer": "^1.0.10",
        "is-plain-object": "^5.0.0",
        "lodash": "^4.17.21",
        "react": ">=16.14.0",
        "swiper": "^8.1.5",
        "twilio": "^3.63.1",
        "twilio-video": "^2.28.0",
        "typescript": "^4.6.2"
    },
    "devDependencies": {
        "@types/d3-timer": "^1.0.9",
        "@types/lodash": "^4.14.199",
        "@types/node": "^18.11.9",
        "@types/react": "^18.2.28",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "react-scripts": "^5.0.1",
        "rimraf": "^5.0.5"
    },
    "peerDependencies": {
        "react": ">=16.14.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
